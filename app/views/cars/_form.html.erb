<%= simple_form_for(car) do |f| %>
<div class = "textforform">
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :mileage %>
  <%= f.input :year %>
  <%= f.input :body_type %>
  <%= f.input :location %>
  <%= f.input :transmission %>
  <%= f.input :price %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit, class: "input-button" %>
</div>
<% end %>
